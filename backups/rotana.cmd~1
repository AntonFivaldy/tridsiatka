# ffmpeg -i %%~Nxi  -vf vflip  -c:a copy    %%~ni%_v.avi
# ffmpeg -i %%~Nxi  -vf hflip  -c:a copy    %%~ni%_HH.avi
# ffmpeg -i %%~ni%_v.avi  -hf vflip  -c:a copy %%~ni%_rotate.avi
# ffmpeg -i %%~Nxi -ss 0:01  -codec:a copy  -codec:v copy  %%~ni%_rotate.avi
