for %%i in (*.mp4) do ((SET FILE_NAME_FULL=%%~Nxi
SET FILE_NAME = %%~ni
SET FILE_NAME_FULL = %%~Nxi
SET FILE_NAME = %%~ni
echo FILE_NAME_FULL = %FILE_NAME_FULL%
echo FILE_NAME = %FILE_NAME%
)
echo FILE_NAME_FULL = %FILE_NAME_FULL%
echo FILE_NAME = %FILE_NAME%
